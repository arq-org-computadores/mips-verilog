                MemRead &lt;= 1&apos;b1;
                RegWrite &lt;= 1&apos;b1;
                Jal &lt;= 1&apos;b0;
                BranchNe &lt;= 1&apos;b0;
                Branch &lt;= 1&apos;b0;
            end
            6&apos;d43: begin // sw
                ALUOp &lt;= 3&apos;b000;
                MemToReg &lt;= 1&apos;bX;
                RegDst &lt;= 2&apos;bXX;
                ALUSrc &lt;= 2&apos;b01;
                MemWrite &lt;= 1&apos;b1;
                MemRead &lt;= 1&apos;b0;
                RegWrite &lt;= 1&apos;b0;
                Jal &lt;= 1&apos;b0;
                BranchNe &lt;= 1&apos;b0;
                Branch &lt;= 1&apos;b0;
            end
            6&apos;d2: begin // j
                ALUOp &lt;= 3&apos;bXXX;
                MemToReg &lt;= 1&apos;bX;
                RegDst &lt;= 2&apos;bXX;
                ALUSrc &lt;= 2&apos;bXX;
                MemWrite &lt;= 1&apos;b0;
                MemRead &lt;= 1&apos;b0;
                RegWrite &lt;= 1&apos;b0;
                Jal &lt;= 1&apos;b0;
                Jump &lt;= 1&apos;b1;
                BranchNe &lt;= 1&apos;bX;
                Branch &lt;= 1&apos;bX;
            end
            6&apos;d3: begin // jal
                ALUOp &lt;= 3&apos;bXXX;
                MemToReg &lt;= 1&apos;bX;
                RegDst &lt;= 2&apos;b10;
                ALUSrc &lt;= 2&apos;bXX;
                MemWrite &lt;= 1&apos;b0;
                MemRead &lt;= 1&apos;b0;
                RegWrite &lt;= 1&apos;b1;
                Jal &lt;= 1&apos;b1;
                Jump &lt;= 1&apos;b1;
                BranchNe &lt;= 1&apos;bX;
                Branch &lt;= 1&apos;bX;
            end
        endcase
    end
endmodule</string>
