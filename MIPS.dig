                RegWrite &lt;= 1&apos;b1;
                Jal &lt;= 1&apos;b0;
                BranchNe &lt;= 1&apos;b0;
                Branch &lt;= 1&apos;b0;
            end
            6&apos;d13: begin // ori
                ALUOp &lt;= 3&apos;b011;
                MemToReg &lt;= 1&apos;b0;
                RegDst &lt;= 2&apos;b00;
                ALUSrc &lt;= 2&apos;b10;
                MemWrite &lt;= 1&apos;b0;
                MemRead &lt;= 1&apos;b0;
                RegWrite &lt;= 1&apos;b1;
                Jal &lt;= 1&apos;b0;
                BranchNe &lt;= 1&apos;b0;
                Branch &lt;= 1&apos;b0;
            end
            6&apos;d14: begin // xori
